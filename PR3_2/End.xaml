<Window x:Class="PR3_2.End"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PR3_2"
        mc:Ignorable="d"
        Title="Завершение" WindowStartupLocation="CenterScreen" MinHeight="450" MinWidth="800" Height="450" Width="800">
    <StackPanel Background="#FF9CD499">
        <Label HorizontalAlignment="Center" FontSize="24">Тест завершен</Label>
        <StackPanel Margin="20,0,20,0" Background="White">
            <Label x:Name="result" Height="180" FontSize="14" Margin="0,10,0,0"></Label>
        </StackPanel>
        <Button Click="reload_Click" Margin="0,10,0,0" Padding="4" HorizontalAlignment="Center" FontSize="24">Начать заново</Button>
    </StackPanel>
</Window>
